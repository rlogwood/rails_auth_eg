
<!-- container mt-w flex flex-row items-center max-w-md mx-auto -->

<nav class="flex flex-row">
  <% if user_signed_in? %>
    <%= link_to "Edit Password", edit_password_path, class: "nav-link" %>
    <%= button_to "Log out", session_path, method: :delete, class: "nav-link" %>
  <% else %>
    <%= link_to "Sign Up", new_registration_path, class: "nav-link" %>
    <%= link_to "Log in", new_session_path, class: "nav-link" %>
  <% end %>
</nav>
